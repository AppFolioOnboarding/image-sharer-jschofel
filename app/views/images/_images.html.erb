<% images.each do |image| %>
  <div class="image-tag-container">
    <%= image_tag image.url %>
    <p>
      <% image.tag_list.each_with_index do |image_tag, index| %>
        <%= link_to image_tag, filter_path(tag: image_tag)  %>
        <%= '-' if index != image.tag_list.length - 1 %>
      <% end %>
    </p>
  </div>
<% end %>
